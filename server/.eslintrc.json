{
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base",
      "plugin:flowtype/recommended",
      "prettier"
    ],
    "plugins": [
      "flowtype",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "prettier/prettier": ["error", {
        "singleQuote": true,
        "trailingComma": "es5"
      }]
    }
}
